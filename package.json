{
  "name": "anstagram",
  "version": "1.0.0",
  "description": "NodeJS 서버 구축 복습을 위한 인스타그램 클론",
  "main": "index.js",
  "scripts": {
    "dev:server": "cd src && nodemon --exec babel-node init.js --delay 2 ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack --watch",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore src/assets --ignore src/static --ignore src/webpack.config.js",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "author": "Jihyun",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.13",
    "autoprefixer": "^10.2.4",
    "aws-sdk": "^2.850.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.9.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "mini-css-extract-plugin": "^1.3.6",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "postcss-loader": "^5.0.0",
    "pug": "^3.0.0",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.1",
    "util": "^0.12.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {}
}
